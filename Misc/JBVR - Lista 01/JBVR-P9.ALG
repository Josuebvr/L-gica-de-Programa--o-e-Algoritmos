Algoritmo "P9"
// Disciplina   : Lógica de Programação e Algoritmos
// Professor   : Sandra


Var

   base:real
   altura:real
   area:real

Inicio

   escreva("Digite o valor da base: ")
   leia(base)

   escreva("Digite o valor da altura: ")
   leia(altura)

   area <- ((base*altura)/2)
   escreva("Area do Triangulo: ", (area))
   leia(area)

Fimalgoritmo