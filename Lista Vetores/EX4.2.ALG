Algoritmo "Ex4.2"

Var
   tab,i,j,resultado: inteiro

Inicio

   //escreval("Digite o Numero da Tabuada: ")
   //leia(tab)
   para i de 1 até 10 faca

      para j de 1 até 10 faca
         resultado<-i * j
         escreval(i," x",j," =",resultado)
      fimpara
      escreval ("")
   fimpara

Fimalgoritmo